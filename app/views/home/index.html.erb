<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#index</th>
        <th scope="col">Team</th>
        <th scope="col">P</th>
        <th scope="col">W</th>
        <th scope="col">L</th>
        <th scope="col">D</th>
        <th scope="col">F</th>
        <th scope="col">A</th>
        <th scope="col">Raw</th>
        <th scope="col">Pts</th>
      </tr>
    </thead>
    <tbody>
      <% if FootBallData.instance.list.present? %>
        <% FootBallData.instance.list.each_with_index do |data, index| %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= data.name %></td>
            <td><%= data.p %></td>
            <td><%= data.w %></td>
            <td><%= data.l %></td>
            <td><%= data.d %></td>
            <td><%= data.f %></td>
            <td><%= data.a %></td>
            <td><%= data.raw %></td>
            <td><%= data.pts %></td>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>